        <function-signature>java.lang.String maxleng( java.lang.String,int  )</function-signature>  
        <example>${my:maxleng('aaaa',2 )}</example>  
    </function> 
    
    <function>  
        <description>json转map</description>  
        <name>showconfig</name>  
        <function-class>com.yebucuo.util.JSTLTagUtils</function-class>  
        <function-signature>java.lang.String showconfig(java.util.List, java.lang.String,java.lang.String  )</function-signature>  
        <example>${my:showconfig(aa,'aaaa','aa')}</example>  
    </function> 
  
