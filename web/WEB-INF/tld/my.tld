        <example>${my:drawpage(page, request, url1 )}</example>  
    </function> 
    
    
    <function>  
        <description>星星</description>  
        <name>stars</name>  
        <function-class>com.yebucuo.util.JSTLTagUtils</function-class>  
        <function-signature>java.lang.String stars(java.lang.String ,float )</function-signature>  
        <example>${my:stars(ctx, 1.0 )}</example>  
    </function> 
    
    <function>  
        <description>获取数字</description>  
        <name>getCount</name>  
        <function-class>com.yebucuo.util.JSTLTagUtils</function-class>  
        <function-signature>java.lang.Integer getCount(long, java.lang.String, java.lang.String  )</function-signature>  
        <example>${my:getCount(1,'user','message' )}</example>  
    </function> 
    
    <function>  
        <description>获取数字</description>  
        <name>getCountTag</name>  
