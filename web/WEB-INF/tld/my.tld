        <function-signature>java.lang.String getCountTag(long, java.lang.String, java.lang.String  )</function-signature>  
        <example>${my:getCountTag(1,'user','message' )}</example>  
    </function> 
    
    <function>  
        <description>截取字符串</description>  
        <name>maxleng</name>  
